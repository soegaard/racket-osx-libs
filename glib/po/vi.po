msgstr "Không thể mở trình chuyển đổi từ '%s' sang '%s'"

#: ../gio/gcontenttype.c:180
msgid "Unknown type"
msgstr "Không rõ kiểu"

#: ../gio/gcontenttype.c:181
#, c-format
msgid "%s filetype"
msgstr "kiểu tập tin %s"

#: ../gio/gcontenttype.c:680
#, c-format
